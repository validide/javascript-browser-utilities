{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "**/*.d.ts",
    "test/**/*.*",
    "dist/**/*.*",
    "coverage/**/*.*",
    ".build/**/*.*"
  ],
  "reporter": [
    "text",
    "lcov"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "check-coverage": true
}
