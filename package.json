{
  "name": "@validide/javascript-browser-utilities",
  "version": "0.0.1",
  "description": "JavaScript Browser Utilities",
  "main": "dist/js/index.js",
  "dependencies": {},
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "4.2.11",
    "@types/jsdom": "16.2.0",
    "@types/mocha": "7.0.2",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "chai": "4.2.0",
    "eslint": "7.26.0",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-jsdoc": "34.8.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "glob": "7.1.6",
    "jsdom": "16.2.2",
    "mocha": "7.1.1",
    "nyc": "15.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.3.3",
    "source-map-support": "0.5.16",
    "ts-node": "8.8.2",
    "typescript": "3.8.3",
    "uglify-js": "3.8.1"
  },
  "scripts": {
    "clean": "node ./.build/clean.js",
    "prebuild": "npm run clean",
    "build": "tsc --module es2015 && rollup --config ./rollup.config.js && node ./.build/minify.js",
    "test": "npm run lint && nyc mocha",
    "predocs": "npm run test && npm run build",
    "docs": "rollup --config ./rollup.config.js --dir ./docs/lib/bundle/",
    "lint": "npm run lint-src && npm run lint-test",
    "lint-src": "eslint -c .eslintrc.js --ext .ts ./src",
    "lint-test": "eslint -c ./test/.eslintrc.js --ext .ts ./test"
  },
  "repository": {
    "type": "git",
